{
  "name": "stocks-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start:intraday": "node --enable-source-maps dist/main.js --mode intraday",
    "start:eod": "node --enable-source-maps dist/main.js --mode eod",
    "dev:intraday": "tsx src/main.ts --mode intraday",
    "dev:eod": "tsx src/main.ts --mode eod",
    "dev:commands": "tsx src/runCommands.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.0",
    "axios": "^1.6.0",
    "better-sqlite3": "^9.4.0",
    "dotenv": "^16.4.0",
    "rss-parser": "^3.12.0",
    "yahoo-finance2": "^2.7.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/yargs": "^17.0.24",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0"
  }
}

